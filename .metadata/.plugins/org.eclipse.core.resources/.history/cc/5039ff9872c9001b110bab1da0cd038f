
public class Chapter05 {

	public static void main(String[] args) {
		long long1 = 10L;
		System.out.println(long1);
		
		double double1 = 1;
		System.out.println(double1);
		
		boolean boolean1 = true;
		System.out.println(boolean1);
		
		Float float1 = 1.1f;
		System.out.println(float1);
		
		
	}
}
