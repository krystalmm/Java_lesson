package practice;

import extend.phone.Phone;

public class FlyingPhone implements Phone, Flying {
	private int min;
	
	public FlyingPhone(int min) {
		this.min = min;
	}
	
	public void fly() {
		System.out.println(this.min + "分間、飛びます。");
	}
	
	public void call(String number) {
		
	}
	
	public void powerOff() {
		
	}
}
